void PrintFrame(FILE *fp, int a)

{ int    d,i,j;
  double y;


  fprintf(fp,"gsave 0.7 setlinewidth\n");

  for(j=0; j<2; j++) /* two angle tables per page */
  { fprintf(fp,"1.2 setlinewidth\n");
    fprintf(fp," 0 %.1f  moveto 486.0 0 rlineto 0 -340.0 rlineto -486.0 0 rlineto closepath stroke\n",732.0-356.0*j);
    fprintf(fp,"  0 %.1f  moveto 486.0 0 rlineto 0 -284.0 rlineto -486.0 0 rlineto closepath stroke\n",704.0-356.0*j);

    /* plot the angled arrows ... */
    fprintf(fp," 21.0 %.1f moveto -10 0 rlineto 0 -12 rlineto stroke\n",696.0-356.0*j);
    fprintf(fp," 11.0 %.1f moveto   2  6 rlineto -4 0 rlineto closepath fill\n",684.0-356.0*j);
    fprintf(fp,"466.0 %.1f moveto  10 0 rlineto 0  12 rlineto stroke\n",427.0-356.0*j);
    fprintf(fp,"476.0 %.1f moveto   2 -6 rlineto -4 0 rlineto closepath fill\n",439.0-356.0*j);

    fprintf(fp,"0.7 setlinewidth\n");

    /* plot the angle values ... */
    fprintf(fp," 243 %.1f  moveto 20 f1 (%d\\260) Cshow\n",710.0-356.0*j,a+j);
    fprintf(fp," 243 %.1f  moveto 20 f1 (%d\\260) Cshow\n",398.0-356.0*j,179-(a+j));

    /* plot the frames ... */
    for(i=0; i<12; i++)
    { if((i%2)==0)
      { if(a+j<3)                            /* grey-out the low-resolution parts of the tables */
        { fprintf(fp,"0.9 setgray\n");
          fprintf(fp,"%.1f %.1f moveto 36.0 0 rlineto 0 -260.0 rlineto -36.0 0 rlineto closepath fill\n",63.0+i*36.0,692.0-356.0*j);
          fprintf(fp,"0.0 setgray\n");
        }
        if(a+j>86)                           /* grey-out the low-resolution parts of the tables */
        { fprintf(fp,"0.9 setgray\n");
          fprintf(fp,"%.1f %.1f moveto 36.0 0 rlineto 0 -260.0 rlineto -36.0 0 rlineto closepath fill\n",27.0+i*36.0,692.0-356.0*j);
          fprintf(fp,"0.0 setgray\n");
        }

        fprintf(fp,"%.1f %.1f moveto 72.0 0 rlineto 0 -284.0 rlineto -72.0 0 rlineto closepath  stroke\n",27.0+i/2*72.0,704.0-356.0*j);
        fprintf(fp,"%.1f %.1f moveto 8 f1 (%02d') Cshow\n",63.0+i/2*72.0,694.0-356.0*j,i*5);
        fprintf(fp,"%.1f %.1f moveto 8 f1 (%02d') Cshow\n",63.0+i/2*72.0,422.0-356.0*j,50-(i*5));
        fprintf(fp,"%.1f %.1f moveto 8 f1 (A\\(x\\)) Cshow\n",45.0+i/2*72.0,683.0-356.0*j,50-(i*5));
        fprintf(fp,"%.1f %.1f moveto 8 f1 (B\\(x\\)) Cshow\n",81.0+i/2*72.0,683.0-356.0*j,50-(i*5));
        fprintf(fp,"%.1f %.1f moveto 8 f1 (A\\(x\\)) Cshow\n",45.0+i/2*72.0,436.0-356.0*j,50-(i*5));
        fprintf(fp,"%.1f %.1f moveto 8 f1 (B\\(x\\)) Cshow\n",81.0+i/2*72.0,436.0-356.0*j,50-(i*5));
      }
      fprintf(fp,"%.1f %.1f moveto 36.0 0 rlineto 0 -260.0 rlineto -36.0 0 rlineto closepath  stroke\n",27.0+i*36.0,692.0-356.0*j);
    }
    fprintf(fp," 27 %.1f  moveto 432.0 0 rlineto 0 -236.0 rlineto -432.0 0 rlineto closepath stroke\n",680.0-356.0*j);

    /* plot the fractional minute values ... */
    y=668.0-j*356.0;
    fprintf(fp,"8 f1\n");
    for(d=0; d<=20; d++)
    { fprintf(fp," 21.0 %.1f moveto (%04.1f) Rshow\n",y,d/2.0);
      fprintf(fp,"466.0 %.1f moveto (%04.1f)  show\n",y,10-d/2.0);
      if((d%3)==2) y-=16.8; else y-=8.4;
    }
  }

  fprintf(fp,"6 f0\n");
  fprintf(fp,"0 17.6 moveto (Warning:) show\n");
  fprintf(fp,"30 17.6 moveto (This page has been generated by a computer program. Complex computer programs usually have bugs and may produce wrong data.) show\n");
  fprintf(fp,"30 8.8 moveto (The data on this page is believed to be accurate but no warranty is given for it's correctness. Use it only for training and exercising! ) show\n");


  fprintf(fp,"grestore\n");

  return;
} 


void MakeTable(FILE *fp, int a, int *page)

{ int    d,j;
  double m,p,q,y;

  fprintf(fp,"%%%%Page: %d %d\n%%%%BeginPageSetup\n40 30 translate\n%%%%EndPageSetup\n",*page,*page);
  PrintFrame(fp,a);

  fprintf(fp,"8 f4\n");
  for(j=0; j<2; j++)
  { for(m=0.0; m<60; m+=10.0)
    { y=668.0-j*356.0;
      for(d=0; d<=20; d++)
      { p=(double)(a+j)+(m+d/2.0)/60.0;
                                                /* calculate the A(x) function */
        if(p>0.0)     
        { q=log10(1/sin(PI/180.0*p));
          fprintf(fp," %.1f %.1f moveto (%d) Rshow\n",60.0+m/10.0*72,y,(int)floor(100000.0*q+0.5));
        }
          else fprintf(fp," %.1f %.1f moveto (-) Rshow\n",60.0+m/10.0*72,y);

                                                /* calculate the B(x) function */
        if(p<90.0)
        { q=log10(1/cos(PI/180.0*p));
          fprintf(fp," %.1f %.1f moveto (%d) Rshow\n",96.0+m/10.0*72,y,(int)floor(100000.0*q+0.5));
        }
          else fprintf(fp," %.1f %.1f moveto (-) Rshow\n",96.0+m/10.0*72,y);
        if((d%3)==2) y-=16.8; else y-=8.4;
      }
    }
  }

  fprintf(fp,"showpage\n");

  *page = (*page)+1;

  return;
}




